
{
    "manifest_version": 2,
  
    "name": "MySocial",
    "description": "Easily view all your social media!",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "storage"]
  }